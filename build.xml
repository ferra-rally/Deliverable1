      <include name="../java-15-openjdk!/jdk.management"/>
      <include name="../java-15-openjdk!/jdk.management.agent"/>
      <include name="../java-15-openjdk!/jdk.management.jfr"/>
      <include name="../java-15-openjdk!/jdk.naming.dns"/>
      <include name="../java-15-openjdk!/jdk.naming.rmi"/>
      <include name="../java-15-openjdk!/jdk.net"/>
      <include name="../java-15-openjdk!/jdk.nio.mapmode"/>
      <include name="../java-15-openjdk!/jdk.sctp"/>
      <include name="../java-15-openjdk!/jdk.security.auth"/>
