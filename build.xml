      <include name="../java-15-openjdk!/jdk.internal.jvmstat"/>
      <include name="../java-15-openjdk!/jdk.internal.le"/>
      <include name="../java-15-openjdk!/jdk.internal.opt"/>
      <include name="../java-15-openjdk!/jdk.internal.vm.ci"/>
      <include name="../java-15-openjdk!/jdk.internal.vm.compiler"/>
      <include name="../java-15-openjdk!/jdk.internal.vm.compiler.management"/>
      <include name="../java-15-openjdk!/jdk.jartool"/>
